{
    "name": "zendframework/skeleton-application",
    "description": "Skeleton Application for Zend Framework zend-mvc applications",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "mvc",
        "zf"
    ],
    "homepage": "http://framework.zend.com/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-component-installer": "^1.0 || ^0.3 || ^1.0.0-dev@dev",
        "zendframework/zend-mvc": "^3.0",
        "zfcampus/zf-development-mode": "^3.0",
        "doctrine/doctrine-orm-module": "^1.1",
        "zendframework/zend-developer-tools": "^1.1",
        "doctrine/migrations": "^1.5",
        "doctrine/data-fixtures": "^1.2",
        "zendframework/zend-json": "^3.0",
        "zendframework/zend-xmlrpc": "^2.6",
        "zendframework/zend-I18n": "^2.7",
        "zendframework/zend-session": "^2.7",
        "zendframework/zend-mvc-plugin-prg": "^1.0",
        "zendframework/zend-mvc-plugin-identity": "^1.0",
        "zendframework/zend-mvc-plugin-flashmessenger": "^1.0",
        "zendframework/zend-mvc-plugin-fileprg": "^1.0",
        "zendframework/zendframework": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/",
            "AccountManager\\": "module/AccountManager/src/",
            "CorporateManager\\": "module/CorporateManager/src/",
            "Blog\\": "module/Blog/src/",
            "StudentManager\\": "module/StudentManager/src/",
            "Accounting\\": "module/Accounting/src/",
            "ProductManager\\": "module/ProductManager/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/",
            "AccountManager\\": "module/AccountManager/test/",
            "CorporateManager\\": "module/CorporateManager/test/",
            "Blog\\": "module/blog/test/",
            "StudentManager\\": "module/StudentManager/test/",
            "Accounting\\": "module/Accounting/test/"
        }
    },
    "extra": [],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php",
        "test": "phpunit"
    }
}